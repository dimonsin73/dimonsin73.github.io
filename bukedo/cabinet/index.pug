    body.cabinet 
        .login 
            .login__left 
                h2.login__title Управляйте вашим магазином
            .login__right 
                a(href="index.html").logo 
                    img(src="img/logo.png", alt="BUCEDO")
                h4.login__right-title Вход в кабинет магазина
                form.login__form 
                    label.login__form-label(for="login-email")  Email
                        input(type="email" placeholder='Email для входа' required).login__form-input#login-email
                    label.login__form-label(for="login-password")  Пароль
                        input(type="password" placeholder='Пароль для входа' required).login__form-input#login-password
                    a(href="").login__forgot Забыли пароль?
                    button(type="submit").catalog.catalog__text Войти
        aside.aside 
            .aside__logo
                a(href="index.html").logo 
                    img(src="img/logo.png", alt="BUCEDO")
            .aside__menu 
                h4.aside__title Магазин
                ul.aside__list
                    li.aside__list-item.aside__list-item-active(data-aside='orders')

doctype html
html(lang="ru")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title IKINGI
        link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css")
        script(src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js" defer)
        link(rel="stylesheet", href="css/reset.css")
        link(rel="stylesheet", href="css/style.css")
        script(type="text/javascript" src="js/script.js" defer) 
    body 
        .hiro
            img(src="images/X7proTrike/TrikeOff1-1.png", alt="").hiro__img
        .menu
            h1.title IKINGI
            button(type="button").btn Купить
        .products
            .swiper
                .swiper-wrapper
                    .swiper-slide 
                        button(type="button" data-scooter='X7proTrike').swiper-scooter
                            img(src="images/X7proTrike/TrikeOff1.png", alt="X7proTrike")
                    .swiper-slide 
                        button(type="button" data-scooter='X12').swiper-scooter
                            img(src="images/X12/1.png", alt="X12")
                    .swiper-slide 
                        button(type="button" data-scooter='X12Trike').swiper-scooter
                            img(src="images/X12Trike/Trike1.png", alt="X12Trike")
